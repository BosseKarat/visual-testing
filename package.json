{
  "name": "@wdio/visual-testing",
  "author": "wswebcreation",
  "description": "Visual testing monorepo for WebdriverIO",
  "license": "MIT",
  "type": "module",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/webdriverio-community/visual-testing.git"
  },
  "scripts": {
    "clean": "rimraf coverage build .tmp",
    "precompile": "npm run clean",
    "test": "vitest",
    "test.eslint": "eslint --cache packages tests",
    "test.local.init": "rimraf localBaseline && wdio ./tests/configs/wdio.local.init.conf.ts",
    "test.local.desktop": "wdio tests/configs/wdio.local.desktop.conf.ts",
    "test.local.appium": "wdio tests/configs/wdio.local.appium.conf.ts",
    "test.local.desktop.multi": "wdio tests/configs/wdio.local.desktop.multi.conf.ts",
    "test.local.dev.tools.init": "wdio ./tests/configs/wdio.local.dev.tools.init.conf.ts",
    "test.local.dev.tools.desktop": "wdio tests/configs/wdio.local.dev.tools.desktop.conf.ts",
    "test.unit.coverage": "vitest --coverage",
    "test.saucelabs": "wdio ./tests/configs/wdio.saucelabs.conf.ts",
    "test.saucelabs.desktop": "SAUCE_ENV=desktop wdio ./tests/configs/wdio.saucelabs.conf.ts",
    "test.saucelabs.emu": "SAUCE_ENV=emu wdio ./tests/configs/wdio.saucelabs.conf.ts",
    "test.saucelabs.sims": "SAUCE_ENV=sims wdio ./tests/configs/wdio.saucelabs.conf.ts",
    "prerelease": "npm run compile",
    "release": "release-it --github.release",
    "release.ci": "npm run release -- --ci --npm.skipChecks --no-git.requireCleanWorkingDir",
    "release.patch": "npm run release -- patch",
    "release.minor": "npm run release -- minor",
    "release.major": "npm run release -- major",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/eslint": "^8.44.9",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@typescript-eslint/utils": "^6.14.0",
    "@vitest/coverage-v8": "^1.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unicorn": "^49.0.0",
    "eslint-plugin-wdio": "^8.24.12",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "release-it": "^17.0.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
